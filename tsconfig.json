{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {    
      "shared.enum/*": ["src/app/shared/enum/*"],
      "shared.guard/*": ["src/app/shared/guard/*"],
      "shared.model/*": ["src/app/shared/model/*"],
      "shared.service/*": ["src/app/shared/service/*"],
      "root.environments/*": ["src/environments/*"]         
    },
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  }
}
